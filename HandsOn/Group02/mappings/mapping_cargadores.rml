<#CargadoresMapping>
    rml:logicalSource [
        rml:source "cargadores-updated.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:object ns:IdPuntoDeRecarga;
    ];
    rr:subjectMap [
        rr:template "http://example.org/cargadores/{OID_}"
    ];
    rr:predicateObjectMap [
        rr:predicate ns:estaEn;
        rr:objectMap [
            rr:parentTriplesMap <#BarriosMapping>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:gestionDe;
        rr:objectMap [
            rml:reference "GESTIÓN"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:tieneOperador;
        rr:objectMap [
            rr:parentTriplesMap <#OperadoresMapping>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:latitud;
        rr:objectMap [
            rml:reference "LATITUD";
            rr:datatype xsd:float
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:longitud;
        rr:objectMap [
            rml:reference "LONGITUD";
            rr:datatype xsd:float
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:numeroEquipos;
        rr:objectMap [
            rml:reference "Nº_EQUIPOS";
            rr:datatype xsd:integer
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ns:esDe;
        rr:objectMap [
            rml:reference "POTENCIA_MAX";
            rr:datatype rdfs:Literal
        ]
    ].

