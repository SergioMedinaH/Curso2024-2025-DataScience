<#IdPuntoDeRecargaClass>
    rml:logicalSource [
        rml:source [];
    ];
    rr:subjectMap [
        rr:constant <http://owl-ontologies.com/ns#IdPuntoDeRecarga>;
    ];
    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:object owl:Class;
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:object "Id Punto de Recarga"@es;
    ].

