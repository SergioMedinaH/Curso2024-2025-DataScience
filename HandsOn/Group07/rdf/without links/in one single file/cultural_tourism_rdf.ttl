<http://turismo.mad.es/cultural/resource/Lighting/plaza-de-canalejas_7> a ns1:Lighting ;
    ns1:hasComment "DISEÑO DE IRIS" ;
    ns1:hasDesigner "BEN BUSCHE" ;
    ns1:hasDistrict <http://turismo.mad.es/cultural/resource/District/centro> ;
