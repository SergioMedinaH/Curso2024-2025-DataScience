    ns1:hasPdf "https://patrimonioypaisaje.madrid.es/FrameWork/generacionPDFMonumenta/408503.pdf?idMonumento=69b8091d1b9c4510091d1b9c45102e085a0aRCRD&tipoMon=M" ;
    ns1:hasSubtype "Elemento conmemorativo, Lápida" ;
