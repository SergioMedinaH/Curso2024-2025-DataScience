    ns1:hasUrl "https://patrimonioypaisaje.madrid.es/sites/v/index.jsp?vgnextchannel=8fac3cb702aa4510VgnVCM1000008a4a900aRCRD&vgnextoid=ea38091d1b9c4510091d1b9c45102e085a0aRCRD"^^xsd:anyURI .

<http://turismo.mad.es/cultural/resource/Monument/eolos> a ns1:Monument ;
    ns1:hasAddress <http://turismo.mad.es/cultural/resource/Address/4480824478956> ;
    ns1:hasAuthor "HOEYDONCK, Paul van" ;
    ns1:hasConstructionDate "1991" ;
    ns1:hasDescription "Se trata de una de las obras, en este caso la diseñada por el escultor belga Paul van Hoeydonck, realizada durante el Simposium Internacional de Esculturas al Aire Libre, denominado “Encuentros en Madrid”, que fue convocado por el Ayuntamiento en el marco de las actividades organizadas para la celebración de la Capitalidad Europea de la Cultura en 1992. En dicho Simposium, cuya primera fase tuvo lugar en 1991, se encargó a 11 artistas de prestigio internacional la realización de sendas macroesculturas, que se ubicarían, siguiendo las indicaciones de emplazamiento previamente establecidas por cada autor, en distintos lugares del parque Juan Carlos I. Actualmente, es una de las piezas (concretamente la n&amp;ordm; 15) que integra la llamada Senda de las Esculturas, un recorrido artístico-didáctico por todo el parque donde el visitante percibe las vinculaciones entre el Arte y la Naturaleza, a través de una serie de creaciones de diferentes escultores que exhiben una gran variedad de propuestas estéticas." ;
    ns1:hasId 409041 ;
    ns1:hasName "eolos" ;
    ns1:hasPdf "https://patrimonioypaisaje.madrid.es/FrameWork/generacionPDFMonumenta/409041.pdf?idMonumento=4b38091d1b9c4510091d1b9c45102e085a0aRCRD&tipoMon=M" ;
