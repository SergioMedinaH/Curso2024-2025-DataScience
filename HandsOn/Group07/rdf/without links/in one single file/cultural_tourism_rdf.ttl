    ns1:hasSubtype "Estatua" ;
    ns1:hasUrl "https://patrimonioypaisaje.madrid.es/sites/v/index.jsp?vgnextchannel=8fac3cb702aa4510VgnVCM1000008a4a900aRCRD&vgnextoid=0918091d1b9c4510091d1b9c45102e085a0aRCRD"^^xsd:anyURI .

<http://turismo.mad.es/cultural/resource/Monument/enrique-larreta> a ns1:Monument ;
