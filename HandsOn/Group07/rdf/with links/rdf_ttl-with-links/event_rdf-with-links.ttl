    ns1:hasPrice "General: 30 euros / Reducida: 27 euros / Amigos FG: 26 euros" ;
    ns1:hasStartDate "2024-11-20T00:00:00+00:00"^^xsd:dateTime ;
    ns1:hasStartTime "20:00:00"^^xsd:time ;
